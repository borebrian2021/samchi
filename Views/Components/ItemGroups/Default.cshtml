

   @model IEnumerable<Umbraco.Cms.Core.Models.PublishedContent.IPublishedContent>


<div class="section">
	<!-- container -->
	<div class="container">
		<!-- row -->
		<div class="row">
			@foreach (var item in Model)
			{
				foreach (var k in item.Children)
				{

					var image = k.Value<IPublishedContent>("categoryIMage");
					<!-- shop -->
					<div class="col-md-4 col-xs-6">
						<div class="shop">
							<div class="shop-img">
								<img class="sample-categories" src="@image.Url()" alt="">
							</div>
							<div class="shop-body">
								<h3>@k.Name<br>Collection</h3>
								<a href="#" class="cta-btn">Shop now <i class="fa fa-arrow-circle-right"></i></a>
							</div>
						</div>
					</div>
				}
			}
				<!-- /shop -->
		</div>
		<!-- /row -->
	</div>
	<!-- /container -->
</div>
